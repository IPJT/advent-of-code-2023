{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "lib": ["esnext"],
    "target": "es2017",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noUncheckedIndexedAccess": true,
    "sourceMap": true,
  }
}
